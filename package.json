{
  "name": "ngx-zendesk-webwidget",
  "version": "0.2.0",
  "main": "./dist/ngx-zendesk-webwidget.js",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-zendesk-webwidget -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --project tsconfig.json src/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlisonVilela/ngx-zendesk-webwidget.git"
  },
  "author": {
    "name": "Alison Vilela",
    "email": "alison.vilela@live.nl"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlisonVilela/ngx-zendesk-webwidget/issues"
  },
  "devDependencies": {
    "@angular/cli": "11.2.1",
    "@angular/common": "11.2.1",
    "@angular/compiler": "11.2.1",
    "@angular/compiler-cli": "11.2.1",
    "@angular/core": "11.2.1",
    "@angular/platform-browser": "11.2.1",
    "@angular/platform-browser-dynamic": "11.2.1",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "3.6.3",
    "@types/node": "14.14.29",
    "codelyzer": "6.0.1",
    "core-js": "3.9.0",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "gulp-rollup": "2.17.0",
    "gulp4-run-sequence": "1.0.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "node-sass": "5.0.0",
    "node-sass-tilde-importer": "1.0.2",
    "node-watch": "0.7.1",
    "protractor": "7.0.0",
    "rollup": "2.39.0",
    "ts-node": "9.1.1",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5",
    "zone.js": "0.11.4"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
